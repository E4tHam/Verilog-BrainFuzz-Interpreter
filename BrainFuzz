#! /usr/local/Homebrew/Cellar/icarus-verilog/10.3/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x7fd7e7c0abb0 .scope module, "brainfuzz_tb" "brainfuzz_tb" 2 5;
 .timescale 0 0;
v0x7fd7e7c06450 .array "RAM", 15 0, 3 0;
v0x7fd7e7c0ad10 .array "ROM", 15 0, 2 0;
v0x7fd7e7c0ada0_0 .var/i "i", 31 0;
    .scope S_0x7fd7e7c0abb0;
T_0 ;
    %vpi_call 2 13 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars" {0 0 0};
    %delay 1, 0;
    %vpi_call 2 18 "$display", "Loading rom..." {0 0 0};
    %vpi_call 2 19 "$readmemb", "ROM.mem", v0x7fd7e7c0ad10 {0 0 0};
    %vpi_call 2 20 "$display", "done!" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fd7e7c0ada0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fd7e7c0ada0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 4;
    %ix/getv/s 4, v0x7fd7e7c0ada0_0;
    %store/vec4a v0x7fd7e7c06450, 4, 0;
    %load/vec4 v0x7fd7e7c0ada0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fd7e7c0ada0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fd7e7c0ada0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x7fd7e7c0ada0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.3, 5;
    %vpi_call 2 27 "$display", &A<v0x7fd7e7c06450, v0x7fd7e7c0ada0_0 > {0 0 0};
    %load/vec4 v0x7fd7e7c0ada0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fd7e7c0ada0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %delay 1, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_0;
